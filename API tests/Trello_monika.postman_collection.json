							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Board name: test_board_1_monika returned in the response\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"test_board_1_monika\");",
